# AWS Configuration
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here
AWS_DEFAULT_REGION=us-east-1

# S3 Configuration
S3_BUCKET_NAME=efference-vlm
S3_DATASET_PREFIX=vlm/dataset_test_s3

# CloudFront Configuration (required for CloudFront presigned URLs)
CLOUDFRONT_DOMAIN=your-distribution.cloudfront.net
CLOUDFRONT_KEY_PAIR_ID=YOUR_KEY_PAIR_ID
CLOUDFRONT_PRIVATE_KEY_PATH=./cloudfront-private-key.pem

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000

# Security
PRESIGNED_URL_EXPIRATION=3600